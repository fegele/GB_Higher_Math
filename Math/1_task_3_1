# график окружности

%matplotlib inline
import matplotlib.pyplot as plt
import math

x = []
y_1 = []
y_2 = []
# радиус
r = 5
# координаты центра окружности
x_0 = 3
y_0 = 2

for i in range(-r * 100, r * 100 + 1):
    x_ = i / 100
    x.append(x_ + x_0)
    y_1.append(math.sqrt(r**2 - x_**2) + y_0)
    y_2.append(-math.sqrt(r**2 - x_**2) + y_0)

plt.plot(x, y_1, color = 'm')
plt.plot(x, y_2, color = 'm')
plt.xlabel('x')
plt.ylabel('y')
plt.axis('equal')
