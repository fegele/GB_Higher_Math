# график эллипсоида

%matplotlib inline
import numpy as np
from pylab import *
from mpl_toolkits.mplot3d import Axes3D

a = 6
b = 7
c = 5

fig = figure()
ax = Axes3D(fig)
X = np.arange(-10, 10, 0.001)
Y = np.arange(-10, 10, 0.001)
X, Y = np.meshgrid(X, Y)
Z1 = np.sqrt((1 - X**2 / a**2 - Y**2 / b**2) / c**2)
Z2 = -np.sqrt((1- X**2 / a**2 - Y**2 / b**2) / c**2)

ax.plot_wireframe(X, Y, Z1, color = 'g')
ax.plot_wireframe(X, Y, Z2, color = 'с')
show()
